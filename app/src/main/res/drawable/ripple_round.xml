<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="?attr/colorControlHighlight">
<item android:id="@android:id/mask">
    <inset
        android:insetLeft="@dimen/rounded_ripple_inset"
        android:insetTop="@dimen/rounded_ripple_inset"
        android:insetRight="@dimen/rounded_ripple_inset"
        android:insetBottom="@dimen/rounded_ripple_inset">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/background_light" />
            <corners android:radius="70dp" />
        </shape>
    </inset>
</item>
<item>
    <inset
        android:insetLeft="@dimen/rounded_ripple_inset"
        android:insetTop="@dimen/rounded_ripple_inset"
        android:insetRight="@dimen/rounded_ripple_inset"
        android:insetBottom="@dimen/rounded_ripple_inset">
        <shape android:shape="rectangle">
            <solid android:color="#1D2633"/>
            <corners android:radius="70dp" />
        </shape>
    </inset>
</item>
</ripple>
