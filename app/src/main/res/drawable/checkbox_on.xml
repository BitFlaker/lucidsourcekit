<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="30dp" android:height="30dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle"
            android:padding="10dp">
            <solid android:color="?attr/colorSecondary" />
            <corners
                android:bottomRightRadius="30dp"
                android:bottomLeftRadius="30dp"
                android:topLeftRadius="30dp"
                android:topRightRadius="30dp" />
            <stroke android:width="0px" android:color="#00000000" />
        </shape>
    </item>
    <item android:drawable="@drawable/ic_baseline_check_white_24" android:width="24dp" android:height="24dp" android:gravity="center"/>
</layer-list>