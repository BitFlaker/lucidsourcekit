<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="16dp" />
            <stroke android:width="1.5dp" android:color="?attr/colorSurfaceContainerLow" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item android:top="1.5dp" android:left="1.5dp" android:right="1.5dp" android:bottom="1.5dp">
        <shape android:shape="rectangle">
            <size android:width="14dp" android:height="14dp" />
            <corners android:radius="16dp" />
            <stroke android:width="1.5dp" android:color="#fff" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
</layer-list>
